# Set PATH so it includes user's private bin if it exists
if [ -d "$HOME/bin" ] ; then
    PATH="$HOME/bin:$PATH"
fi
PATH="$HOME/.local/bin":$PATH


# Added by Miniconda3 installer
export PATH="$HOME/miniconda3/bin:$PATH"

# `jupyter`
if [[ -z $(ps -u $(whoami) | grep "jupyter") ]]; then
	nohup jupyter notebook --no-browser --port=7777 --port-retries=0 &> .jupyter_log&
fi
